class A is
    var a : Integer()
    method setA(v: Integer) : Integer is
        a := v
        setB(5)
        return a
    end
    method setB(v: Integer) : Integer is
        a := v
        return a
    end
    this() is end
end

class B extends A is
    var b : Integer()
    this(x :Integer, y :Integer) is
        a := x
        b := y
    end
end

class Main is
    this() is
        var x : B(1, 2)
        x.setA(5)
    end
end
